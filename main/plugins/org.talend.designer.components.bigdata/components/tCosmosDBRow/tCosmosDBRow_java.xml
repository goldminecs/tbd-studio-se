<COMPONENT>
  	<HEADER
	    PLATEFORM="ALL"
	    SERIAL=""
	    VERSION="0.101"
	    STATUS="ALPHA"

	    COMPATIBILITY="ALL"
	    AUTHOR="Talend"
	    RELEASE_DATE="20170608A"
	    STARTABLE="true"
		PARTITIONING="NONE"
		LOG4J_ENABLED="true"
  	>
    	<SIGNATURE/>
  	</HEADER>
	<FAMILIES>
		<FAMILY>Cloud/Azure/Cosmos DB</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
	    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
	    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
	    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
	    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
	    <CONNECTOR CTYPE="COMPONENT_OK"/>
	    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
	    <CONNECTOR CTYPE="RUN_IF"/>
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER
			NAME="USE_EXISTING_CONNECTION"
			FIELD="CHECK"
			NUM_ROW="2"
			GROUP="CONNECTION"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER
			NAME="CONNECTION"
			FIELD="COMPONENT_LIST"
			REQUIRED="true" FILTER="tCosmosDBConnection"
			NUM_ROW="2"
			SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
			GROUP="CONNECTION"
		/>
        <PARAMETER
            NAME="API"
            FIELD="CLOSED_LIST"
            NUM_ROW="3"
            SHOW_IF="(USE_EXISTING_CONNECTION == 'false')"
            GROUP="CONNECTION"
        >
            <ITEMS DEFAULT="MONGODB">
                <ITEM NAME="MONGODB" VALUE="MONGODB" />
            </ITEMS>
        </PARAMETER>
		<PARAMETER
            FIELD="CHECK"
            NAME="USE_REPLICA_SET"
            NUM_ROW="4"
            SHOW_IF="(USE_EXISTING_CONNECTION == 'false')"
            GROUP="CONNECTION">
            <DEFAULT>"false"</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="REPLICA_SET"
            FIELD="TABLE"
            REQUIRED="true"
            NUM_ROW="5"
            NB_LINES="5"
            SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (USE_REPLICA_SET=='true')"
            GROUP="CONNECTION">
            <ITEMS>
                <ITEM NAME="REPLICA_HOST" FIELD="TEXT" />
                <ITEM NAME="REPLICA_PORT" FIELD="TEXT" />
            </ITEMS>
        </PARAMETER>
		<PARAMETER
			FIELD="TEXT"
			NAME="HOST"
			NUM_ROW="6"
			REQUIRED="true"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (USE_REPLICA_SET=='false')"
			GROUP="CONNECTION"
		>
			<DEFAULT>"localhost"</DEFAULT>
		</PARAMETER>
		<PARAMETER
			FIELD="TEXT"
			NAME="PORT"
			NUM_ROW="6"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (USE_REPLICA_SET=='false')"
			GROUP="CONNECTION"
		>
			<DEFAULT>10255</DEFAULT>
		</PARAMETER>
		<PARAMETER
			FIELD="TEXT"
			NAME="DATABASE"
			NUM_ROW="7"
			REQUIRED="true"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			GROUP="CONNECTION"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
        <PARAMETER FIELD="CHECK" NAME="USE_SSL" NUM_ROW="8" SHOW="false" GROUP="CONNECTION">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>

		<PARAMETER
			NAME="REQUIRED_AUTHENTICATION"
			FIELD="CHECK"
			NUM_ROW="12"
			SHOW="false"
			GROUP="AUTHENTICATION"
		>
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

        <PARAMETER NAME="AUTHENTICATION_MECHANISM" FIELD="CLOSED_LIST" NUM_ROW="15"
                SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (REQUIRED_AUTHENTICATION == 'true')" GROUP="AUTHENTICATION">
            <ITEMS DEFAULT="NEGOTIATE_MEC">
                <ITEM NAME="NEGOTIATE_MEC" VALUE="NEGOTIATE_MEC"/>
                <ITEM NAME="PLAIN_MEC" VALUE="PLAIN_MEC"/>
                <ITEM NAME="SCRAMSHA1_MEC" VALUE="SCRAMSHA1_MEC"/>
            </ITEMS>
        </PARAMETER>
        
        <PARAMETER FIELD="CHECK" NAME="SET_AUTHENTICATION_DATABASE"
            NUM_ROW="18" SHOW_IF="((AUTHENTICATION_MECHANISM=='NEGOTIATE_MEC') OR (AUTHENTICATION_MECHANISM=='SCRAMSHA1_MEC')) AND (REQUIRED_AUTHENTICATION == 'true') AND (USE_EXISTING_CONNECTION == 'false')" GROUP="AUTHENTICATION">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>

        <PARAMETER FIELD="TEXT" NAME="AUTHENTICATION_DATABASE" NUM_ROW="19"
            SHOW_IF="(SET_AUTHENTICATION_DATABASE=='true') AND (isShow[SET_AUTHENTICATION_DATABASE])" GROUP="AUTHENTICATION">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>

        <PARAMETER
            FIELD="TEXT"
            NAME="USERNAME"
            NUM_ROW="20"
            SHOW_IF="((AUTHENTICATION_MECHANISM=='NEGOTIATE_MEC') OR (AUTHENTICATION_MECHANISM=='PLAIN_MEC') OR (AUTHENTICATION_MECHANISM=='SCRAMSHA1_MEC')) AND (REQUIRED_AUTHENTICATION == 'true') AND (USE_EXISTING_CONNECTION == 'false')"
            GROUP="AUTHENTICATION"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER
            FIELD="PASSWORD"
            NAME="PASSWORD"
            NUM_ROW="20"
            SHOW_IF="((AUTHENTICATION_MECHANISM=='NEGOTIATE_MEC') OR (AUTHENTICATION_MECHANISM=='PLAIN_MEC') OR (AUTHENTICATION_MECHANISM=='SCRAMSHA1_MEC')) AND (REQUIRED_AUTHENTICATION == 'true') AND (USE_EXISTING_CONNECTION == 'false')"
            GROUP="AUTHENTICATION"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>

		<PARAMETER
			NAME="SCHEMA_FLOW"
			FIELD="SCHEMA_TYPE"
			NUM_ROW="30"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER
			NAME="EXECUTE_COMMAND"
			FIELD="CHECK"
			NUM_ROW="35"
			SHOW_IF="false"
		>
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		<PARAMETER
			NAME="EXECUTE_KEYVALUECOMMAND"
			FIELD="CHECK"
			NUM_ROW="36"
			SHOW_IF="(EXECUTE_COMMAND == 'true') AND (EXECUTE_JSONCOMMAND == 'false')"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER
			NAME="EXECUTE_JSONCOMMAND"
			FIELD="CHECK"
			NUM_ROW="37"
			SHOW_IF="(EXECUTE_COMMAND == 'true') AND (EXECUTE_KEYVALUECOMMAND == 'false')"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER
			FIELD="TEXT"
			NAME="COMMAND"
			REQUIRED="true"
			NUM_ROW="40"
			SHOW_IF="(EXECUTE_COMMAND == 'true') AND (EXECUTE_KEYVALUECOMMAND == 'false') AND (EXECUTE_JSONCOMMAND == 'false')"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER
			NAME="COMMAND_KEYVALUES"
			FIELD="TABLE"
			REQUIRED="false"
			NUM_ROW="41"
			NB_LINES="5"
			SHOW_IF="(EXECUTE_COMMAND =='true') AND (EXECUTE_KEYVALUECOMMAND == 'true')"
		>
			<ITEMS>
				<ITEM NAME="KEY" />
				<ITEM NAME="VALUE" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER
			FIELD="LABEL"
			NAME="JSON_COMMAND_NOTE"
			REQUIRED="false"
			NUM_ROW="42"
			SHOW_IF="(EXECUTE_COMMAND =='true') AND (EXECUTE_JSONCOMMAND == 'true')"
		>
			<DEFAULT>Note : Surround String values with single quotes or escaped double quotes</DEFAULT>
		</PARAMETER>

		<PARAMETER
			FIELD="MEMO"
			NAME="JSON_COMMAND"
			REQUIRED="false"
			NUM_ROW="43"
			NB_LINES="5"
			SHOW_IF="(EXECUTE_COMMAND =='true') AND (EXECUTE_JSONCOMMAND == 'true')"
		>
			<DEFAULT>"{createIndexes: 'restaurants', indexes : [{key : {restaurant_id: 1}, name: 'id_index_2', unique: true}]}"</DEFAULT>
		</PARAMETER>

		<PARAMETER
			FIELD="MEMO"
			NAME="FUNCTION"
			REQUIRED="true"
			NUM_ROW="45"
			NB_LINES="5"
			SHOW_IF="(EXECUTE_COMMAND =='false')"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="FUNCTION_PARAMETERS"
			FIELD="TABLE"
			REQUIRED="false"
			NUM_ROW="50"
			NB_LINES="5"
			SHOW_IF="(EXECUTE_COMMAND =='false')"
		>
			<ITEMS>
				<ITEM NAME="PARAMETER" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER
			NAME="DIE_ON_ERROR"
			FIELD="CHECK"
			NUM_ROW="55">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	</PARAMETERS>

	<ADVANCED_PARAMETERS />

	<CODEGENERATION>
		<IMPORTS>
            
            <IMPORT MODULE="mongo-java-driver-3.4.2.jar" MVN="mvn:org.talend.libraries/mongo-java-driver-3.4.2/6.0.0"  NAME="mongo-java-driver-3.4.2" UrlPath="platform:/plugin/org.talend.libraries.mongo/lib/mongo-java-driver-3.4.2.jar" REQUIRED_IF="(USE_EXISTING_CONNECTION == 'false') AND (API=='MONGODB')" />

        </IMPORTS>
	</CODEGENERATION>
	<RETURNS/>
</COMPONENT>
